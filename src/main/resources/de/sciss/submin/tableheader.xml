<skin>

    <!-- com.alee.laf.table.WebTableHeader -->

    <!-- Table header -->
    <style type="tableheader">
        <component>
            <opaque>true</opaque>
        </component>
        <painter>
            <headerHeight>20</headerHeight>
            <topLineColor>134,134,134</topLineColor>
            <bottomLineColor>48,48,48</bottomLineColor>
            <topBgColor>126,126,126</topBgColor>
            <bottomBgColor>101,101,101</bottomBgColor>
            <gridColor>160,160,160,128</gridColor>
            <borderColor>160,160,160,128</borderColor>
        </painter>

        <!-- Default table header cell renderer label -->
        <style type="label" id="renderer" extends="shade" padding="0,10,1,10">
            <component>
                <opaque>true</opaque>
                <horizontalAlignment>0</horizontalAlignment>
            </component>
            <painter class="SubminLabelPainter">
                <shadeColor>160,160,160</shadeColor>
                <shadeSize>1</shadeSize>
            </painter>
            <!--<painter class="SubminLabelPainter">-->
                <!--<shadeColor>160,160,160</shadeColor>-->
                <!--<shadeSize>1</shadeSize>-->
                <!--<shadeShadow>true</shadeShadow>-->
                <!--<shadeShiftY>1</shadeShiftY>-->
            <!--</painter>-->
        </style>

    </style>

</skin>