<skin>

    <!-- com.alee.laf.tabbedpane.WebTabbedPane -->

    <!-- Tabbed pane -->
    <style type="tabbedpane">
        <ui>
            <tabbedPaneStyle>standalone</tabbedPaneStyle>
        </ui>
        <painter>
            <round>2</round>
            <shadeWidth>2</shadeWidth>
            <selectedTopBg>144,144,144</selectedTopBg>
            <selectedBottomBg>180,180,180</selectedBottomBg>
            <topBg>126,126,126</topBg>
            <bottomBg>101,101,101</bottomBg>
            <tabBorderColor>84,84,84</tabBorderColor>
            <contentBorderColor>180,180,180</contentBorderColor>
            <backgroundColor>180,180,180</backgroundColor>
            <paintBorderOnlyOnSelectedTab>false</paintBorderOnlyOnSelectedTab>
            <forceUseSelectedTabBgColors>true</forceUseSelectedTabBgColors>
            <paintOnlyTopBorder>false</paintOnlyTopBorder>
        </painter>
    </style>

    <!-- Attached tabbed pane -->
    <style type="tabbedpane" id="attached">
        <ui>
            <tabbedPaneStyle>attached</tabbedPaneStyle>
        </ui>
    </style>

</skin>